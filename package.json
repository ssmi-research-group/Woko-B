{
  "name": "suitablebot",
  "version": "1.1.0",
  "private": true,
  "description": "A GitHub app to show suitable people to participate in an issue",
  "author": "Gabriel Chaves",
  "license": "ISC",
  "repository": "https://github.com/GaMoCh/suitablebot.git",
  "homepage": "https://github.com/GaMoCh/suitablebot",
  "bugs": "https://github.com/GaMoCh/suitablebot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "probot": "^9.5.3",
    "remove-markdown": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^24.9.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": "12.x"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
